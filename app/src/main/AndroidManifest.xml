<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ethan627hsu.wake.wake">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_Yellow">
        <activity android:name=".mainui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".player.PlayerActivity" />
        <activity android:name=".editor.EditorActivity" />
        <activity android:name=".editor.SubEditorActivity"
            android:theme="@style/AppTheme_Red"></activity>
    </application>

</manifest>